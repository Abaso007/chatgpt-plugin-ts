{
  "compilerOptions": {
		"target": "es2021",
    "lib": ["esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "outDir": "build",
    "noEmit": true,
    "types": [
			"@cloudflare/workers-types"
		]
  },
  "exclude": ["node_modules", "build"],
  "include": ["**/*.ts"]
}
